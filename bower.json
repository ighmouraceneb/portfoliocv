{
  "name": "website",
  "homepage": "https://github.com/thomasbatt/portfoliocv",
  "authors": [
    "thomasbatt <thomasbatt@gmail.com>"
  ],
  "description": "Cv portfolio de Thomas BATT",
  "main": "index.php",
  "keywords": [
    "Developpeur",
    "Integrateur",
    "Web",
    "Strasbourg"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap-sass": "^3.3.6",
    "devicons": "^1.8.0",
    "devicon": "*",
    "font-awesome": "fontawesome#^4.6.3",
    "wow": "wowjs#^1.1.2",
    "animate.css": "^3.5.2",
    "angular": "1.5.0",
    "ngSticky": "^1.8.6",
    "angular-parallax": "^0.0.2",
    "angular-scroll": "^1.0.0",
    "elh-tooltip": "*"
  },
  "main": {
    "bowerPath":"bower_components", 
    "destPath": "assets",
    "destFile": "vendors",
    "srcPath": "./"
  },
  "sassPaths": [
    "bootstrap-sass/assets/stylesheets",
    "animate.css",
    "devicons/css",
    "devicon",
    "elh-tooltip",
    "font-awesome/scss"
  ],
  "jsVendorsFiles": [
    "angular/angular.min.js",
    "angular-parallax/scripts/angular-parallax.js",
    "angular-scroll/angular-scroll.min.js",
    "jquery/dist/jquery.min.js",
    "ngSticky/dist/sticky.min.js",
    "wow/dist/wow.min.js"
  ],
  "iconsFiles": [
    "bootstrap-sass/assets/fonts/*/**.*",
    "devicons/fonts/**.*",
    "devicon/fonts/**.*",
    "font-awesome/fonts/**.*"
  ]
}
